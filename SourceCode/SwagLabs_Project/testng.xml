<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Cross-Browser Test Suite" parallel="classes" thread-count="2">
	<test name="ChromeTest">
		<parameter name="browser" value="chrome" />
		<classes>
		<class name="com.qa.saucedemo.practical.runners.TestRunner" />
		</classes>
	</test>
	<test name="EdgeTest">
		<parameter name="browser" value="edge" />
		<classes>
			<class name="com.qa.saucedemo.practical.runners.TestRunner" />
		</classes>
	</test>
</suite> 